    "babel": "babel ./source --out-dir ./lib",
    "build": "cross-env NODE_ENV=production gulp",
    "clean": "rimraf ./lib",
    "css": "cpx \"./source/**/*.css\" ./lib",
    "lint": "npm run lint:js && npm run lint:css",
